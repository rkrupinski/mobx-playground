{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "no-underscore-dangle": "off",
    "react/no-unused-prop-types": "warn", // https://github.com/yannickcr/eslint-plugin-react/issues/847#issuecomment-247737320
    "react/prefer-stateless-function": "off",
    "react/sort-comp": "off"
  },
  "extends": "airbnb"
}
